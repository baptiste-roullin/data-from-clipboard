{
  "name": "data-from-clipboard",
  "repository": "https://github.com/baptiste-roullin/data-from-clipboard",
  "description": "Populate a bunch of layers from a list of text in the clipboard",
  "version": "0.6.1",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "data-from-clipboard",
    "manifest": "src/manifest.json",
    "main": "data-from-clipboard.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.8.0",
    "sketch-utils": "^0.2.10"
  },
  "author": "Baptiste <shakespearien@gmail.com>"
}